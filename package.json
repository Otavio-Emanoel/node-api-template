{
  "name": "node-api-template",
  "version": "1.0.0",
  "description": "The ultimate production-ready boilerplate for Node.js APIs. Fastify, Prisma, TypeScript, Zod, Vitest & Docker. Batteries included.",
  "keywords": [
    "nodejs",
    "typescript",
    "fastify",
    "prisma",
    "boilerplate",
    "clean-architecture",
    "docker",
    "vitest"
  ],
  "homepage": "https://github.com/Otavio-Emanoel/node-api-template#readme",
  "bugs": {
    "url": "https://github.com/Otavio-Emanoel/node-api-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Otavio-Emanoel/node-api-template.git"
  },
  "license": "ISC",
  "author": "Otavio-Emanoel",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "dev": "npx ts-node-dev src/server.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down -v",
    "postinstall": "prisma generate && node scripts/prisma-link.mjs",
    "test": "vitest"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@prisma/adapter-pg": "^7.2.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "pg": "^8.16.3",
    "prisma": "^7.2.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@prisma/client": "^7.2.0",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
